# This file is structured like a multi stage build

# The order here is significant, index 0 is the default EE
FROM quay.io/ansible/creator-ee:v0.9.2

# Index 1 is the small image and should be a base image for the default EE
FROM quay.io/ansible/python-base:latest

# Index 2 is simply a pullable image for tests
FROM registry.hub.docker.com/library/alpine:latest
